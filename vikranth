#include<iostream>
using namespace std;
int main()
{
    int a[10][10],i,j,m,n;
    cout<<" Enter the size of columns and rows"<<endl;
    cin>>m>>n;
    cout<<" Enter the elements of matrix"<<endl;
    for(i=0;i<m;i++)
    for(j=0;j<n;j++)
    cin>>a[i][j];
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        cout<<a[i][j];
        cout<<endl;
    }
}
